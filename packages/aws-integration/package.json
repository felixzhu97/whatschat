{
  "name": "@whatschat/aws-integration",
  "version": "0.1.0",
  "private": true,
  "description": "AWS 集成工具包 - 提供 S3、SES、SNS、SQS、Lambda、Cognito 和 CloudWatch 等 AWS 服务的封装",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-sesv2": "^3.0.0",
    "@aws-sdk/client-sns": "^3.0.0",
    "@aws-sdk/client-sqs": "^3.0.0",
    "@aws-sdk/client-lambda": "^3.0.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.0.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.0.0",
    "@aws-sdk/client-cloudwatch": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "@aws-sdk/types": "^3.0.0",
    "@aws-sdk/smithy-client": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^22",
    "tsup": "^8.0.0",
    "typescript": "5.8.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "keywords": [
    "aws",
    "s3",
    "ses",
    "sns",
    "sqs",
    "lambda",
    "cognito",
    "cloudwatch",
    "integration"
  ],
  "author": "felix zhu <z1434866867@gmail.com>",
  "license": "MIT"
}